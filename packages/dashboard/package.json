{
  "name": "proof-of-innocence-dashboard",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run reset-cache && craco start",
    "dev": "craco start",
    "reset-cache": "rm -rf node_modules/.cache",
    "build-dev": "PUBLIC_URL=/ NODE_ENV=development REACT_APP_DEV_BUILD=1 craco build",
    "build-prod": "PUBLIC_URL=/ craco build",
    "build-electron": "craco build",
    "eslint": "eslint src --ext .ts,.tsx --fix",
    "check-circular-deps": "madge --circular --extensions ts,tsx src/**/*",
    "lint": "npm run eslint; npm run tsc;",
    "tsc": "tsc --noEmit",
    "compile-test": "npm run tsc",
    "test": "npm run compile-test && NODE_ENV=test mocha",
    "postinstall": "yarn lint"
  },
  "dependencies": {
    "@railgun-community/shared-models": "5.6.2",
    "axios": "0.27.2",
    "classnames": "^2.3.2",
    "crypto-browserify": "^3.12.0",
    "ethers": "github:Railgun-Community/ethers.js#v6.7.6",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-datepicker": "^4.11.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.6.0",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "6.11.1",
    "react-select": "5.7.3",
    "react-textarea-autosize": "^8.4.0",
    "react-tiny-popover": "^7.2.4",
    "react-tooltip": "5.13.0",
    "sass": "^1.67.0",
    "spinners-react": "^1.0.7",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "7.21.5",
    "@craco/craco": "^7.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.6.2",
    "@types/node-sass": "^4.11.4",
    "@types/psl": "^1.1.0",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "18.2.6",
    "@types/react-datepicker": "^4.11.2",
    "@types/react-dom": "18.2.4",
    "@types/react-modal": "3.16.0",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "craco-alias": "^3.0.1",
    "esbuild-loader": "^3.0.1",
    "eslint": "8.40.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "5.11.0",
    "madge": "6.0.0",
    "mocha": "^10.2.0",
    "prettier": "2.8.8",
    "punycode": "^2.1.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "sass": "1.62.1",
    "tsc-alias": "1.8.6",
    "typescript": "5.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
