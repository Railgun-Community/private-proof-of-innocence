{
  "name": "@railgun-community/proof-of-innocence",
  "version": "2.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16.20.1"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "start": "npm run build && env DEBUG=poi:* node dist/main.js",
    "start-list-provider": "npm run build && env DEBUG=poi:* LIST_PROVIDER=1 node dist/main.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "check-circular-deps": "madge --circular .",
    "eslint": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "npm run eslint && tsc --noEmit && npm run check-circular-deps && tsc --noEmit -p tsconfig.test.json",
    "tsc-test": "npx --node-options='--max-old-space-size=16000' tsc -p tsconfig.test.json && tsc-alias -p tsconfig.test.json",
    "compile-test": "npm run clean && npm run tsc-test",
    "test-coverage": "npm run compile-test && c8 mocha",
    "prod-db": "./run-mongodb-prod",
    "test-db": "./run-mongodb-test",
    "setup-env": "node setup-env.js",
    "generate-keys": "node ./src/config/keyGenerator.js",
    "test": "npm run compile-test && NODE_ENV=test mocha",
    "test-debug": "env DEBUG=poi:* npm run test"
  },
  "dependencies": {
    "@noble/ed25519": "^1.7.1",
    "axios": "^1.5.0",
    "bits-to-bytes": "^1.3.0",
    "bloom-filters": "^3.0.0",
    "circomlibjs": "hsg88/circomlibjs#ffjavascrip.0.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-json-validator-middleware": "^3.0.1",
    "javascript-time-ago": "^2.5.9",
    "mongodb": "^6.0.0",
    "snarkjs": "^0.7.0"
  },
  "peerDependencies": {
    "@railgun-community/shared-models": "5.8.7",
    "@railgun-community/wallet": "5.14.17",
    "ethers": "github:Railgun-Community/ethers.js#v6.7.8"
  },
  "devDependencies": {
    "@railgun-community/shared-models": "5.8.7",
    "@railgun-community/wallet": "5.14.17",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/json-schema": "^7.0.13",
    "@types/leveldown": "^4.0.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.2",
    "@types/sinon": "^10.0.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "c8": "^8.0.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "ethers": "github:Railgun-Community/ethers.js#v6.7.8",
    "leveldown": "^6.1.1",
    "madge": "^6.1.0",
    "mocha": "^10.2.0",
    "mongoose": "^7.5.0",
    "pm2": "^5.3.0",
    "prettier": "^3.0.0",
    "sinon": "^15.2.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  }
}
